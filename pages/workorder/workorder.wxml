<!--pages/workorder/workorder.wxml-->
<view class="list" style=" background-color:#D7DBDD">
  <view class="item" wx:for="{{process}}" wx:key="item" style="display:flex; height: 200rpx;background-color: white;margin-top: 5px; justify-content: space-between;">
    <view style="display: flex;  flex-direction: column;">
        <text class="plan_name">{{item[0]}}</text>
        <text class="linename">{{item[1]}}</text>
        <text class="quantity">{{item[2]}}</text>
        <text class="sch_date">{{item[3]}}</text>
    </view>
    <view>
      
    </view>    
  </view>
</view>